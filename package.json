{
  "name": "harneet-language-support",
  "repository":"https://github.com/harneetlang/vscode",
  "displayName": "Harneet Language Support",
  "description": "Syntax highlighting and language configuration for the Harneet (.ha) language",
  "version": "0.0.1",
  "publisher": "harneet",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "",
  "contributes": {
    "languages": [
      {
        "id": "harneet",
        "aliases": ["Harneet", "harneet"],
        "extensions": [".ha"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "harneet",
        "scopeName": "source.harneet",
        "path": "./syntaxes/harneet.tmLanguage.json"
      }
    ]
  }
}
